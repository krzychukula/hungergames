<!DOCTYPE html>
<html>
  <head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>{{ title }}</h1>
    <p>Welcome to {{ title }}</p>


    <a href="/auth/google">Login Google</a>

    <a href="/account/unlink/google">Unlink Google</a>

    <a href="/account">/account</a>

    <a href="/logout">/logout</a>

    {{#messages.errors}}
    {{> msg}}
    {{/messages.errors}}

    {{#messages.info}}
    {{> msg}}
    {{/messages.info}}

    {{#messages.success}}
    {{> msg}}
    {{/messages.success}}
  </body>
</html>
