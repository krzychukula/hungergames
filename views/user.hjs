<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>{{ title }}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
  <h1>{{ title }}</h1>
  <!-- Welcome {{ user }} -->
  <p> {{ user.email }}</p>
  <p> {{ user.prifile.name }}</p>
  <p> {{ user.prifile.gender }}</p>
  <p> {{ user.prifile.location }}</p>
  <p> {{ user.prifile.website }}</p>

  <a href="/">Home</a>

  <a href="/logout">/logout</a>


  {{#messages.errors}}
  {{> msg}}
  {{/messages.errors}}

  {{#messages.info}}
  {{> msg}}
  {{/messages.info}}

  {{#messages.success}}
  {{> msg}}
  {{/messages.success}}
</body>
</html>
